/**
  *    cfg/proj.h
  * 
  *    This include file sets firmware options at compile time.
  *    DO NOT MANUALLY SET VERSION NUMBERS HERE! Use the CMake project
  *    VERSION definition instead!
  * 
  */

#ifndef PICOPOST_PROJ_CFG
#define PICOPOST_PROJ_CFG

// Main firmware major version
#define PROJ_MAJ_VER   @pico_post_fw_VERSION_MAJOR@

// Main firmware minor version
#define PROJ_MIN_VER   @pico_post_fw_VERSION_MINOR@

// POST code database version
#define PROJ_CDB_VER   @pico_post_fw_VERSION_PATCH@

#define STR_HELPER(x) #x
#define STR(x) STR_HELPER(x)

#define PROJ_STR_VER   STR(PROJ_MAJ_VER) "." STR(PROJ_MIN_VER) "." STR(PROJ_CDB_VER)

#endif // PICOPOST_PROJ_CFG
